{
  "name": "fundbio-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma db push --accept-data-loss && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:security": "vitest run src/test/security/auth.test.ts src/test/security/vulnerability.test.ts",
    "security:monitor": "node scripts/security-monitor.js",
    "deploy:staging": "./scripts/railway-deploy.sh -e staging",
    "deploy:production": "./scripts/railway-deploy.sh -e production",
    "migrate:dev": "./scripts/migrate-database.sh -e development",
    "migrate:staging": "./scripts/migrate-database.sh -e staging",
    "migrate:production": "./scripts/migrate-database.sh -e production",
    "postinstall": "prisma generate && prisma db push --accept-data-loss",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:seed:complete": "npx tsx prisma/seed-complete.ts"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.14.0",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query": "^5.45.0",
    "@trpc/client": "^11.0.0-next-beta.334",
    "@trpc/next": "^11.0.0-next-beta.334",
    "@trpc/react-query": "^11.0.0-next-beta.334",
    "@trpc/server": "^11.0.0-next-beta.334",
    "@types/leaflet": "^1.9.19",
    "autoprefixer": "^10.4.21",
    "leaflet": "^1.9.4",
    "leaflet.markercluster": "^1.5.3",
    "lucide-react": "^0.525.0",
    "next": "^14.2.3",
    "next-auth": "^4.24.7",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-leaflet": "^4.2.1",
    "react-leaflet-markercluster": "^4.0.0",
    "recharts": "^3.0.2",
    "server-only": "^0.0.1",
    "shpjs": "^6.1.0",
    "superjson": "^2.2.1",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "axe-core": "^4.10.3",
    "critters": "^0.0.23",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.3",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.38",
    "prisma": "^5.14.0",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.20.3",
    "typescript": "^5.4.5",
    "vitest": "^3.2.4"
  }
}
